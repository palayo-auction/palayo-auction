{
  "analysis": {
    "analyzer": {
      "nori_analyzer": {
        "type": "custom",
        "tokenizer": "nori_tokenizer",
        "filter": [
          "lowercase",
          "nori_part_of_speech",
          "korean_stop"
        ]
      }
    },
    "filter": {
      "korean_stop": {
        "type": "stop",
        "stopwords": "_korean_"
      }
    }
  }
}
